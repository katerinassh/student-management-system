<form action = "/api/students"method="POST" id="add_user">
    <div class="form">
        <div class="sign-up-container">
            <div class="form-group up-sign">
                <label for="name" class="form-name">Name</label>
                <input type="text" name="name" class="sign" autocomplete="off"
                    placeholder="Ex: Mark Stoenis">
            </div>
            <div class="form-group up-sign">
                <label for="Faculty" class="form-faculty">Faculty</label>
                <input type="faculty" name="faculty"" class="sign" autocomplete="off"
                placeholder="IASA">
            </div>
            <div class="form-group up-sign">
                <label for="Group" class="form-group">Group</label>
                <input type="group" name="group" class="sign" autocomplete="off"
                    placeholder="DA-XX">
            </div>
            <div class="form-group up-sign">
                <label for="Email" class="form-email">Email </label>
                <input type="email" name="email" class="sign" autocomplete="off"
                    placeholder="example@gmail.com">
            </div>
            <div class="form-group up-sign">
                <label for="Phone" class="form-phone">Phone</label>
                <input type="phone" name="phone" class="sign" autocomplete="off"
                    placeholder="0981234567">
            </div>
            <div class="form-group up-sign">
                <label for="Record Book" class="form-url">Record Book</label>
                <input type="url" name="url" class="sign" autocomplete="off"
                    placeholder="./record-books/..">
            </div>
            <div class="form-group up-sign">
                <label for="Birthday" class="form-birthday">Birthday</label>
                <input type="birthday" name="birthday" class="sign" autocomplete="off"
                    placeholder="2002-12-09">
            </div>
            <div class="form-btn form-group">
                <button class="sign-up-submit-btn update-btn save">Registrate</button>
            </div>
        </div>
    </div>
</form>